{
    "name": "public-accountability/pai-packages",
    "description": "WordPress packages for public-accountability.org",
    "type": "project",
    "license": "GPLv3",
    "authors": [
        {
            "name": "Pea",
            "email": "pea@misfist.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
              "name": "misfist/littlesis",
                "version": "v0.1.11",
                "type": "wordpress-theme",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/public-accountability/littlesis-news-theme/archive/master.zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/public-accountability/littlesis-news-theme.git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
              "name": "misfist/littlesis-core-functionality",
                "version": "0.1.4",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/public-accountability/littlesis-core-functionality/archive/master.zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/public-accountability/littlesis-core-functionality.git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
              "name": "misfist/pai",
                "version": "0.1.0",
                "type": "wordpress-theme",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/public-accountability/pai/archive/master.zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/public-accountability/pai.git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
              "name": "misfist/pai-core-functionality",
                "version": "0.1.0",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/public-accountability/pai-core-functionality/archive/master.zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/public-accountability/pai-core-functionality.git",
                    "reference": "master"
                }
            }
        },
        {
          "type": "package",
          "package": {
            "name": "advanced-custom-fields/advanced-custom-fields-pro",
            "version": "5.6.9",
            "type": "wordpress-plugin",
            "dist": {
              "type": "zip",
              "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download"
            },
            "require": {
              "philippbaschke/acf-pro-installer": "^1.0",
              "composer/installers": "^1.0"
            }
          }
        }
    ],
    "require": {
      "advanced-custom-fields/advanced-custom-fields-pro": "^5.6.9",
      "wpackagist-plugin/caldera-forms": "^1.5.0",
      "wpackagist-plugin/co-authors-plus": "^3.2.1",
      "wpackagist-plugin/codepress-admin-columns": "^3.0.3",
      "wpackagist-plugin/easy-table-of-contents": "^1.2",
      "wpackagist-plugin/google-analytics-dashboard-for-wp": "^5.1.1.1",
      "wpackagist-plugin/jetpack": "^5.3",
      "wpackagist-plugin/mailchimp-for-wp": "^4.1.8",
      "wpackagist-plugin/post-type-archive-links": "^1.3.1",
      "wpackagist-plugin/series": "^2.0.0",
      "wpackagist-plugin/shortcode-ui": "^0.7.0",
      "wpackagist-plugin/widget-css-classes": "^1.4.0",
      "misfist/pai-core-functionality": "^0.1.0",
      "wpackagist-theme/understrap": "^0.5.6",
      "misfist/pai": "^0.1.0",
      "misfist/littlesis-core-functionality": "^0.1.4",
      "misfist/littlesis": "^0.1.11"
    },
    "require-dev": {
      "wpackagist-plugin/developer": "^1.2.5",
      "wpackagist-plugin/term-management-tools": "^1.1.4",
      "wpackagist-plugin/transients-manager": "^1.7.3",
      "wpackagist-plugin/user-switching": "^1.1.0",
      "wpackagist-plugin/which-template": "^4.0",
      "wpackagist-plugin/wordpress-importer": "^0.6.3",
      "wpackagist-plugin/wp-log-viewer": "^1.0.7"
    },
    "extra": {
      "installer-paths": {
        "../wordpress/wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
        "../wordpress/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
        "../wordpress/wp-content/themes/{$name}/": ["type:wordpress-theme"]
      }
    }
}
